<table>
    <tr>
    <th>id</th>
    <th>odate</th>
    <th>adate</th>
    <th>status</th>
    <th>desc</th>
    </tr>
    <% for a in @activity_arr %>

        <tr>
            <td><%= a[:order_id] %></td>
            <td><%= a[:odate] %></td>
            <td><%= a[:adate] %></td>
            <td><%= a[:status] %></td>
            <td><%= a[:activity_title].html_safe %></td>
        </tr>

    <% end %>
</table>